Algoritmo Segun_Switch
	
	dayWeek <- 'JUEVES'
	
	// segun / switch 
	Segun dayWeek Hacer
		"LUNES":
			Escribir "Primer dia de la semana"
		"MARTES":
			Escribir "Segundo dia de la semana"
		"MIERCOLES":
			Escribir "Tercer dia de la semana"
		De Otro Modo:
			Escribir "No se reconoce el día de la semana"
	Fin Segun
	
	
	
FinAlgoritmo
